<interface name="com.deepin.daemon.Grub2">
  <method name="GetAvailableResolutions">
    <arg name="modeJSON" type="s" direction="out"/>
  </method>
  <method name="GetSimpleEntryTitles">
    <arg name="titles" type="as" direction="out"/>
  </method>
  <method name="GetThemes">
    <arg name="themes" type="s" direction="out"/>
  </method>
  <method name="Reset"/>
  <method name="SetDefaultEntry">
    <arg name="entry" type="s" direction="in"/>
  </method>
  <method name="SetEnableTheme">
    <arg name="enabled" type="b" direction="in"/>
  </method>
  <method name="SetResolution">
    <arg name="resolution" type="s" direction="in"/>
  </method>
  <method name="SetTheme">
    <arg name="themeId" type="s" direction="in"/>
  </method>
  <method name="SetTimeout">
    <arg name="timeout" type="u" direction="in"/>
  </method>
  <property name="EnableTheme" type="b" access="read"/>
  <property name="Theme" type="s" access="read"/>
  <property name="Resolution" type="s" access="read"/>
  <property name="Timeout" type="u" access="read"/>
  <property name="Updating" type="b" access="read"/>
  <property name="DefaultEntry" type="s" access="read"/>
</interface>
